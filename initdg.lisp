(defparameter *longcode-s* '("75.5")) ;simple list of keys

;mit maplist
;unf setf überlegn, könnte gehen
(defun longcode-fns-t (slst) ; for simple list
  (mapcar (lambda (ai) ; alist-item
            (lambda (strg)
              (if (ppcre:scan (format nil "^~a\\d" ai) strg)
                (ppcre:regex-replace (cdr ai) strg "")
                strg)))
          slst))

;geht nicht !!
; oder create a alist form simple lsit with gethash
; cdr alist is header and not in off-ht
(defun alist-from-lst (lst)
  (mapcar (lambda (x)
            (cons x (gethash x off-ht)))
          lst))


